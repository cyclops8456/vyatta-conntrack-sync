multi:
type: txt
help: Protocol for which expect entries need to be synchronized.
syntax:expression: pattern $VAR(@) "^(all|ftp|sip|h323|NFS|SQL*net)$"; "Invalid protocol name $VAR(@)"
create: expression: "sudo /opt/vyatta/sbin/vyatta-conntrack-sync.pl --enable"
delete: expression: "sudo /opt/vyatta/sbin/vyatta-conntrack-sync.pl --disable"
